<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="org.tmatz.pocketmushroom" android:versionCode="1"
	android:versionName="1.0">
	<application android:icon="@drawable/icon" android:label="@string/app_name">
		<activity android:name="pocketmushroom.MushroomActivity" android:icon="@drawable/icon"
			android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name="pocketmushroom.MushroomActivity" android:icon="@drawable/icon"
			android:label="@string/app_name">
			<intent-filter>
				<action android:name="com.adamrocker.android.simeji.ACTION_INTERCEPT" />
				<category android:name="com.adamrocker.android.simeji.REPLACE" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
		<activity android:name="pocketmushroom.LoginActivity" android:icon="@drawable/icon"
			android:label="@string/app_name">
			<intent-filter>
				<action android:name="org.tmatz.pocketmushroom.ACTION_LOGIN" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
	</application>
	<uses-sdk
		android:minSdkVersion="8"
		android:targetSdkVersion="11" />
</manifest>
